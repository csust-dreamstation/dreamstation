package com.ds.bean;

public class Spirit {
private int id;
private String title;

private String spirit;

public int getId() {
	return id;
}

public void setId(int id) {
	this.id = id;
}

public String getTitle() {
	return title;
}

public void setTitle(String title) {
	this.title = title;
}

public String getSpirit() {
	return spirit;
}

public void setSpirit(String spirit) {
	this.spirit = spirit;
}

}
